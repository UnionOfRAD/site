						<section id="return">
				<h3 class="h-beta">Returns</h3>
				<span class="type">string</span>
									<span class="return">
						<p>Returns the first template file found. Throws an exception if no templates
are available.</p>
					</span>
							</section>
			
			
			
			
			
						<section id="source" class="section">
				<h3 class="h-beta">Source</h3>
				<div id="source" class="source-display">
					<div class="source-wrapper">
						<pre class="source-code"><code class="language-php">	protected function _paths($type, array $params) {
		if (!isset($this-&gt;_paths[$type])) {
			throw new TemplateException(&quot;Invalid template type &#039;{$type}&#039;.&quot;);
		}

		foreach ((array) $this-&gt;_paths[$type] as $path) {
			if (!file_exists($path = Text::insert($path, $params))) {
				continue;
			}
			return $path;
		}
		throw new TemplateException(&quot;Template not found at path `{$path}`.&quot;);
	}</code></pre>
